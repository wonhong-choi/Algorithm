   "outputs": [],
   "source": [
    "# naive solution\n",
    "# T/C: O(n^2)\n",
    "# S/C: O(1)\n",
    "def get_min_diff(nums):\n",
    "    if len(nums)<2:\n",
    "        return float('inf')\n",
    "    min_diff=float('inf')\n",
    "    for i in range(len(nums)-1):\n",
    "        for j in range(i+1,len(nums)):\n",
    "            min_diff=min(min_diff,abs(nums[i]-nums[j]))\n",
    "    return min_diff"
   ]
