   "source": [
    "# general solution \n",
    "# T/C: O(n+k)\n",
    "# S/C: O(n+k) # for count and tmp array\n",
    "def counting_sort1(nums,k):\n",
    "    count=[0]*k\n",
    "    for num in nums:\n",
    "        count[num]+=1\n",
    "    for i in range(1,len(count)):\n",
    "        count[i]+=count[i-1]\n",
    "    \n",
    "    #print(count)\n",
    "    \n",
    "    tmp=[0]*len(nums)\n",
    "    for i in range(len(nums)-1,-1,-1):\n",
    "        tmp[count[nums[i]]-1]=nums[i]\n",
    "        count[nums[i]]-=1\n",
    "    for i in range(len(nums)):\n",
    "        nums[i]=tmp[i]"
   ]
  },
