   "metadata": {},
   "outputs": [],
   "source": [
    "# T/C: O(nlogn)\n",
    "# S/C: O(1)\n",
    "def maximum_guests(arrive,departure):\n",
    "    arrive.sort()\n",
    "    departure.sort()\n",
    "    max_guests, cur_guests=0,0\n",
    "    i,j=0,0\n",
    "    while i<len(arrive) and j<len(departure):\n",
    "        if arrive[i]<=departure[j]:\n",
    "            i+=1\n",
    "            cur_guests+=1\n",
    "        else:\n",
    "            j+=1\n",
    "            cur_guests-=1\n",
    "        max_guests=max(max_guests,cur_guests)\n",
    "    return max_guests"
   ]
  },
