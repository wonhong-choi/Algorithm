   "outputs": [],
   "source": [
    "# using sorting\n",
    "# T/C: O(nlogn)\n",
    "# S/C: O(1)\n",
    "def merge_intervals(intervals):\n",
    "    merged=[]\n",
    "    intervals.sort(key=lambda x:x[0])\n",
    "    cur=intervals[0]\n",
    "    for i in range(1,len(intervals)):\n",
    "        if cur[1]<intervals[i][0]:\n",
    "            merged.append(cur[:])\n",
    "            cur=intervals[i]\n",
    "        elif cur[1]<intervals[i][1]:\n",
    "            cur[1]=intervals[i][1]\n",
    "    merged.append(cur[:])\n",
    "    return merged"
   ]
