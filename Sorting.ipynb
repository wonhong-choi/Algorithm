  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "372c72d7",
   "metadata": {},
   "outputs": [],
   "source": [
    "# dutch national flag algorithm\n",
    "# T/C: O(n)\n",
    "# S/C: O(1)\n",
    "def sort_colors(nums):\n",
    "    i,j,k=0,0,len(nums)-1\n",
    "    while j<k:\n",
    "        if nums[j]==0:\n",
    "            nums[i],nums[j]=nums[j],nums[i]\n",
    "            j+=1\n",
    "            i+=1\n",
    "        elif nums[j]==2:\n",
    "            nums[j],nums[k]=nums[k],nums[j]\n",
    "            k-=1\n",
    "        else:\n",
    "            j+=1"
   ]
