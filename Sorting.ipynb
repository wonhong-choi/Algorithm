   "id": "4343a432",
   "metadata": {},
   "outputs": [],
   "source": [
    "# T/C: O(n)\n",
    "# S/C: O(1)\n",
    "def three_way_partition(nums,pivot):\n",
    "    i,j,k=0,0,len(nums)-1\n",
    "    while j<k:\n",
    "        if nums[j]<pivot:\n",
    "            nums[i],nums[j]=nums[j],nums[i]\n",
    "            i+=1\n",
    "            j+=1\n",
    "        elif nums[j]>pivot:\n",
    "            nums[j],nums[k]=nums[k],nums[j]\n",
    "            k-=1\n",
    "        else:\n",
    "            j+=1"
   ]
  },
