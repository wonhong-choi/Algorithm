   "source": [
    "# naive solution # 4 pass\n",
    "# T/C: O(n)\n",
    "# S/C: O(n)\n",
    "def sort_colors1(nums):\n",
    "    tmp=[]\n",
    "    for num in nums:\n",
    "        if num==0:\n",
    "            tmp.append(num)\n",
    "    for num in nums:\n",
    "        if num==1:\n",
    "            tmp.append(num)\n",
    "    for num in nums:\n",
    "        if num==2:\n",
    "            tmp.append(num)\n",
    "    for i in range(len(nums)):\n",
    "        nums[i]=tmp[i]"
   ]
