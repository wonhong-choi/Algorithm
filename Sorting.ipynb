  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "cb9d49a1",
   "metadata": {},
   "outputs": [],
   "source": [
    "# T/C: O(nlogn)\n",
    "# S/C: O(n), because space allocated and then, deallocated after sorted\n",
    "def merge_sort(nums,l,r):\n",
    "    if r>l: # at least 2 elements\n",
    "        mid=l+(r-l)//2\n",
    "        merge_sort(nums,l,mid)\n",
    "        merge_sort(nums,mid+1,r)\n",
    "        merge(nums,l,mid,r)        "
   ]
