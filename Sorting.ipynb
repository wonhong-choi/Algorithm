   "outputs": [],
   "source": [
    "# for not duplicated\n",
    "def cycle_sort(nums):\n",
    "    for cycle_start in range(len(nums)-1):\n",
    "        item=nums[cycle_start]\n",
    "        pos=cycle_start\n",
    "        for i in range(cycle_start+1,len(nums)):\n",
    "            if item>nums[i]:\n",
    "                pos+=1\n",
    "        nums[pos],item=item,nums[pos]\n",
    "        while pos!=cycle_start:\n",
    "            pos=cycle_start\n",
    "            for i in range(cycle_start+1,len(nums)):\n",
    "                if item>nums[i]:\n",
    "                    pos+=1\n",
    "            nums[pos],item=item,nums[pos]"
   ]
