   "source": [
    "# T/C: O(n)\n",
    "# S/C: O(1)\n",
    "# pivot element may not be placed partition position\n",
    "# example, [5,3,8,4,2,7,1,10] ==> [1,3,2,4,8,7,5,10]\n",
    "def partition3(nums,low,high):\n",
    "    pivot=nums[low]\n",
    "    i,j=low-1,high+1\n",
    "    while True:\n",
    "        while True:\n",
    "            i+=1\n",
    "            if nums[i]>=pivot:\n",
    "                break\n",
    "        while True:\n",
    "            j-=1\n",
    "            if nums[j]<=pivot:\n",
    "                break\n",
    "        if i>=j:\n",
    "            return j\n",
    "        nums[i],nums[j]=nums[j],nums[i]\n"
   ]
